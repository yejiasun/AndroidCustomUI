<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tengteng.ui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CustomUI">
        <activity android:name="com.tengteng.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tengteng.ui.text.TextActivity" />
        <activity android:name="com.tengteng.ui.recycleview.RecycleViewActivity" />
        <activity android:name="com.tengteng.ui.fish.FishActivity" />
        <activity android:name="com.tengteng.ui.flowlayout.FlowLayoutActivity" />
    </application>

</manifest>